<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.izenesoft.com" xmlns:t="http://www.izenesoft.com" targetNamespace="http://www.izenesoft.com" elementFormDefault="qualified">
    <xs:include schemaLocation="common.xsd"/>

    <xs:complexType name="DocumentMiningParaType">
        <xs:attribute name="cron" type="xs:string" use="optional" />
        <xs:attribute name="docnumlimit" use="optional">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="10000"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="TaxonomyParaType">
        <xs:attribute name="candlabelnum" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="100"/>
                    <xs:maxInclusive value="400"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="enablenec" type="YesNoType" use="required"/>
        <xs:attribute name="levels" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="3"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="maxlocnum" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="50"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="maxorgnum" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="50"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="maxpeopnum" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="50"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="perlevelnum" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="2"/>
                    <xs:maxInclusive value="20"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="topdocnum" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="50"/>
                    <xs:maxInclusive value="300"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="AutofillParaType">
        <xs:attribute name="cron" type="xs:string" use="required" />
        <xs:attribute name="enableUpdateHitnum" type="YesNoType" use="optional" />
    </xs:complexType>
    <xs:complexType name="FuzzyIndexMergeParaType">
        <xs:attribute name="cron" type="xs:string" use="required" />
    </xs:complexType>
    <xs:complexType name="RecommendParaType">
        <xs:attribute name="cron" type="xs:string" use="required" />
        <xs:attribute name="recommendnum" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="50"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="SimilarityParaType">
        <xs:attribute name="docnumlimit" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="100"/>
                    <xs:maxInclusive value="500"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="termnumlimit" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="100"/>
                    <xs:maxInclusive value="500000"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="enableesa" type="YesNoType" use="optional"/>
    </xs:complexType>
    <xs:complexType name="ClassificationParaType">
        <xs:attribute name="customizetraining" type="YesNoType" use="required"/>
        <xs:attribute name="trainingencoding" type="EncodingType" use="required"/>
    </xs:complexType>
    <xs:complexType name="IseParaType">
        <xs:attribute name="buildimageindex" type="YesNoType" use="required"/>
        <xs:attribute name="maximagenum" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="1000000"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="relatedimagenum" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="100"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="storeimagelocally" type="YesNoType" use="required"/>
    </xs:complexType>
    <xs:complexType name="QueryCorrectionParaType">
        <xs:attribute name="enableCN" type="YesNoType" use="required"/>
        <xs:attribute name="enableEK" type="YesNoType" use="required"/>
        <xs:attribute name="fromDb" type="YesNoType" use="optional"/>
    </xs:complexType>
    <xs:complexType name="ProductRankingParaType">
        <xs:attribute name="cron" type="xs:string" use="required" />
    </xs:complexType>
    <xs:complexType name="MiningTaskParaType">
        <xs:attribute name="threadnum" use="optional">
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="50"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>

    <xs:element name="MiningBundle">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Schema" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="TaxonomyGeneration" minOccurs="0"/>
                            <xs:element ref="DuplicateDetection" minOccurs="0"/>
                            <xs:element ref="QueryRecommend" minOccurs="0"/>
                            <xs:element ref="Similarity" minOccurs="0"/>
                            <xs:element ref="DocumentClassification" minOccurs="0"/>
                            <xs:element ref="Faceted" minOccurs="0"/>
                            <xs:element ref="Group" minOccurs="0"/>
                            <xs:element ref="Attr" minOccurs="0"/>
                            <xs:element ref="ProductRanking" minOccurs="0"/>
                            <xs:element ref="TDT" minOccurs="0"/>
                            <xs:element ref="Summarization" minOccurs="0"/>
                            <xs:element ref="SuffixMatch" minOccurs="0"/>
                            <xs:element ref="ProductMatcher" minOccurs="0"/>
                            <xs:element ref="QueryIntent" minOccurs="0"/>
                            <xs:element ref="Zambezi" minOccurs="0"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="Parameter" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="CollectionDataDirectory" type="CollectionDataDirectoryType" minOccurs="0" maxOccurs="2"/>
                            <xs:element name="DocumentMiningPara" type="DocumentMiningParaType" minOccurs="0"/>
                            <xs:element name="TaxonomyPara" type="TaxonomyParaType" minOccurs="0"/>
                            <xs:element name="AutofillPara" type="AutofillParaType" minOccurs="0"/>
                            <xs:element name="FuzzyIndexMergePara" type="FuzzyIndexMergeParaType" minOccurs="0"/>
                            <xs:element name="RecommendPara" type="RecommendParaType" minOccurs="0"/>
                            <xs:element name="SimilarityPara" type="SimilarityParaType" minOccurs="0"/>
                            <xs:element name="ClassificationPara" type="ClassificationParaType" minOccurs="0"/>
                            <xs:element name="IsePara" type="IseParaType" minOccurs="0"/>
                            <xs:element name="QueryCorrectionPara" type="QueryCorrectionParaType" minOccurs="0"/>
                            <xs:element name="ProductRankingPara" type="ProductRankingParaType" minOccurs="0"/>
                            <xs:element name="MiningTaskPara" type="MiningTaskParaType" minOccurs="0"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <xs:element name="TaxonomyGeneration">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Property" type="PropertyType" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="kpe_only" type="YesNoType"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="DuplicateDetection">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Property" type="PropertyType" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="fp_only" type="YesNoType"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="QueryRecommend">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="TG" minOccurs="0"/>
                <xs:element name="Property" type="PropertyType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="QueryLog" minOccurs="0"/>
                <xs:element ref="AutoFill" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="TG">
        <xs:complexType/>
    </xs:element>
    <xs:element name="QueryLog">
        <xs:complexType/>
    </xs:element>
    <xs:element name="AutoFill">
        <xs:complexType>
            <xs:attribute name="days" type="xs:positiveInteger" use="optional" />
        </xs:complexType>
    </xs:element>

    <xs:element name="Similarity">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Property" type="PropertyType" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="Summarization">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="DocidProperty" type="PropertyType" minOccurs="0" maxOccurs="1"/>
                <xs:element name="UuidProperty" type="PropertyType" minOccurs="0" maxOccurs="1"/>
                <xs:element name="ContentProperty" type="PropertyType" minOccurs="0" maxOccurs="1"/>
                <xs:element name="AdvantageProperty" type="PropertyType" minOccurs="0" maxOccurs="1"/>
                <xs:element name="DisadvantageProperty" type="PropertyType" minOccurs="0" maxOccurs="1"/>
                <xs:element name="TitleProperty" type="PropertyType" minOccurs="0" maxOccurs="1"/>
                <xs:element name="ScoreProperty" type="PropertyType" minOccurs="0" maxOccurs="1"/>
                <xs:element name="CommentCountProperty" type="PropertyType" minOccurs="0" maxOccurs="1"/>
                <xs:element name="OpinionProperty" type="PropertyType" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="OpinionWorkingPath" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="OpinionSyncId" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        <xs:attribute name="syncflagonly" type="YesNoType" use="optional"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="SuffixMatch">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Property" type="PropertyType" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="TokenizeDictionary" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="Incremental" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="GroupCounterTopK" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="FilterProperty" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="QueryIntent">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Property" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="Zambezi">
        <xs:complexType>
            <xs:attribute name="debug" type="YesNoType" use="optional"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="TokenizeDictionary">
        <xs:complexType>
            <xs:attribute name="path" type="xs:string" use="required" />
        </xs:complexType>
    </xs:element>

    <xs:element name="Incremental">
        <xs:complexType>
            <xs:attribute name="enable" type="YesNoType" use="required" />
        </xs:complexType>
    </xs:element>

    <xs:element name="GroupCounterTopK">
        <xs:complexType>
            <xs:attribute name="num" type="xs:integer" use="required" />
        </xs:complexType>
    </xs:element>

    <xs:element name="FilterProperty">
        <xs:complexType>
            <xs:attribute name="name" type="xs:string" use="required" />
            <xs:attribute name="filtertype" type="xs:string" use="required" />
            <xs:attribute name="searchable" type="YesNoType" use="optional" />
            <xs:attribute name="amplifier" type="xs:integer" use="optional" />
        </xs:complexType>
    </xs:element>


    <xs:element name="OpinionWorkingPath">
        <xs:complexType>
            <xs:attribute name="path" type="xs:string" use="required" />
        </xs:complexType>
    </xs:element>

    <xs:element name="OpinionSyncId">
        <xs:complexType>
            <xs:attribute name="name" type="xs:string" use="required" />
        </xs:complexType>
    </xs:element>

    <xs:element name="DocumentClassification">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Property" type="PropertyType" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="Faceted">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Property" type="PropertyType" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="Group">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Property" type="GroupPropertyType" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="Attr">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Property" type="PropertyType" minOccurs="1" maxOccurs="1"/>
                <xs:element name="Exclude" type="ExcludeType" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="ProductRanking">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Score" type="ScoreType" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="debug" type="YesNoType" use="optional"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="TDT">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="TokenizeDictionary" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        <xs:attribute name="enabletdt" type="YesNoType" use="required"/>
        <xs:attribute name="enablesemantic" type="YesNoType" use="optional"/>
        <xs:attribute name="tdtmemory" type="TdtStorage" use="optional"/>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="GroupPropertyType">
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="rebuild" type="YesNoType" use="optional"/>
    </xs:complexType>

    <xs:complexType name="ExcludeType">
        <xs:attribute name="name" type="xs:string" use="optional"/>
    </xs:complexType>

    <xs:complexType name="ScoreType">
        <xs:sequence>
            <xs:element name="Score" type="ScoreType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="type" type="xs:string" use="optional"/>
        <xs:attribute name="property" type="xs:string" use="optional"/>
        <xs:attribute name="weight" type="xs:float" use="optional"/>
        <xs:attribute name="min" type="xs:float" use="optional"/>
        <xs:attribute name="max" type="xs:float" use="optional"/>
        <xs:attribute name="debug" type="YesNoType" use="optional"/>
    </xs:complexType>

    <xs:element name="ProductMatcher">
        <xs:complexType>
            <xs:attribute name="mode" type="xs:string" use="optional" />
        </xs:complexType>
    </xs:element>

</xs:schema>
