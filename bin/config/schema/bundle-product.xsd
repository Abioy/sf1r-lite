<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.izenesoft.com" xmlns:t="http://www.izenesoft.com" targetNamespace="http://www.izenesoft.com" elementFormDefault="qualified">
    <xs:include schemaLocation="common.xsd"/>

        <xs:element name="ProductBundle">
            <xs:complexType>
                <xs:sequence>
                    <xs:element name="Schema" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element ref="PriceProperty" minOccurs="1" maxOccurs="1" />
                                <xs:element ref="DateProperty" minOccurs="1" maxOccurs="1" />
                                <xs:element ref="DOCIDProperty" minOccurs="1" maxOccurs="1" />
                                <xs:element ref="UuidProperty" minOccurs="1" maxOccurs="1" />
                                <xs:element ref="ItemCountProperty" minOccurs="1" maxOccurs="1" />
                                <xs:element ref="PriceTrend" minOccurs="0" maxOccurs="1" />
                                <xs:element ref="Backup" minOccurs="0" maxOccurs="1" />
                                <xs:element ref="UuidMap" minOccurs="0" maxOccurs="1" />
                            </xs:sequence>
                            <xs:attribute name="mode" use="required">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="m|a"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Parameter" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="CollectionDataDirectory" type="CollectionDataDirectoryType" minOccurs="1" maxOccurs="2"/>
                                <xs:element ref="CronPara" minOccurs="0"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
        </xs:element>
        <xs:element name="PriceProperty" type="PropertyType" />
        <xs:element name="DateProperty" type="PropertyType" />
        <xs:element name="DOCIDProperty" type="PropertyType" />
        <xs:element name="UuidProperty" type="PropertyType" />
        <xs:element name="ItemCountProperty" type="PropertyType" />
        <xs:element name="PriceTrend">
            <xs:complexType>
                <xs:sequence>
                    <xs:element name="GroupProperty" type="PropertyType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="TimeInterval" minOccurs="0" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:attribute name="days" type="xs:positiveInteger" use="required" />
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
        </xs:element>
        <xs:element name="Backup">
            <xs:complexType>
                <xs:attribute name="path" type="xs:string" use="required" />
            </xs:complexType>
        </xs:element>
        <xs:element name="UuidMap">
            <xs:complexType>
                <xs:attribute name="path" type="xs:string" use="required" />
            </xs:complexType>
        </xs:element>

</xs:schema>
